#!/bin/bash
g++ -std=c++2a -march=native -fopenmp -fsanitize=address -O3 -o ./camera ./camera.cc
g++ -S   -fsanitize=address -march=native  -fopenmp -O3 -o ./Assembly.txt ./camera.cc
./camera

